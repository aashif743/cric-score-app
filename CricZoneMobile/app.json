{
  "expo": {
    "name": "CricZone",
    "slug": "CricZoneMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0d3b66"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.criczone.mobile",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "CricZone needs camera access for profile photos.",
        "NSPhotoLibraryUsageDescription": "CricZone needs photo library access to select profile pictures.",
        "CFBundleDisplayName": "CricZone"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0d3b66"
      },
      "package": "com.criczone.mobile",
      "edgeToEdgeEnabled": true,
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    },
    "owner": "your-expo-username",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/your-project-id"
    }
  }
}
